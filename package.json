  "main": "dist/src/index.js",
  "scripts": {
    "clean": "npx rimraf dist/*",
    "copy-assets": "npx ts-node scripts/copyAssets",
    "lint": "npx tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npx npm-run-all clean lint tsc copy-assets",
    "dev:start": "npx npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "cy:run": "cypress run && ./scripts/stopDocker.sh",
    "start:docker": "./scripts/startDocker.sh",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start:docker http://localhost:8080 cy:run"
  },
  "dependencies": {
    "ejs": "^3.1.3",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.13",
    "@types/shelljs": "^0.8.8",
    "cypress": "^4.8.0",
    "fs-extra": "^9.0.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.8",
    "shelljs": "^0.8.4",
    "start-server-and-test": "^1.11.0",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
